    "electron-prebuilt": "^1.2.8",
    "immutable": "^3.8.1",
    "lodash": "^4.13.1",
    "material-ui": "^0.15.2",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-immutable-proptypes": "^1.7.1",
    "react-redux": "^4.4.5",
